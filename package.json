    "serve-spm": "^0.5.1",
    "serve-static": "^1.2.1",
    "serve-favicon": "^2.0.1",
    "tarball-extract": "~0.0.3",
    "tempfile": "~1.0.0",
    "spm-builder": "*",
    "tar": "~1.0.1",
    "fstream": "~1.0.2",
