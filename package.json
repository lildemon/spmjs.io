{
  "name": "spmjs.io",
  "descrition": "The packaging server for spm@3.x",
  "private": true,
  "version": "0.0.2",
  "author": "afc163 <afc163@gmail.com>",
  "keywords": [
    "spm",
    "package",
    "cmd",
    "browser",
    "commonjs",
    "seajs",
    "static",
    "component"
  ],
  "homepage": "http://spmjs.io",
