  },
  "licenses": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "postinstall": "tar -xzvf elastic/elasticsearch.tar.gz",
    "start": "./node_modules/forever/bin/forever start app.js && nohup elasticsearch-1.3.0/bin/elasticsearch -p .spmjspid >/dev/null 2>&1 &",
    "stop": "./node_modules/forever/bin/forever stop app.js && kill -9 `cat .spmjspid`",
    "reindex": "node elastic/reindexPackages.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.3.0",
    "capitalize": "^0.5.0",
    "connect-multiparty": "^1.0.3",
    "cookie-parser": "^1.1.0",
    "debug": "^1.0.4",
    "ejs": "*",
    "elastical": "0.0.13",
    "errorhandler": "^1.0.0",
    "express": "^4.4.1",
    "express-session": "^1.2.0",
    "fs-extra": "^0.11.0",
    "github-token": "~0.1.0",
    "githuburl": "^0.1.6",
    "highlight.js": "^8.0.0",
    "lodash": "^2.4.1",
    "marked": "^0.3.2",
    "mkdirp": "^0.5.0",
    "moment": "^2.6.0",
    "morgan": "^1.1.0",
    "ms": "^0.6.2",
    "nedb": "~0.10.5",
